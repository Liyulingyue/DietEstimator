{
  "default_branch": "main",
  "default_interval": "10m",
  "jobs": [
    {
      "name": "dietestimator",
      "repo_path": "..",
      "interval": "10m",
      "post_update": [
        {
          "cmd": ["docker-compose", "-f", "docker/docker-compose.dev.yml", "down"],
          "cwd": ".."
        },
        {
          "cmd": ["docker-compose", "-f", "docker/docker-compose.dev.yml", "up", "-d", "db", "adminer"],
          "cwd": ".."
        },
        {
          "cmd": ["docker-compose", "-f", "docker/docker-compose.dev.yml", "up", "-d", "--build"],
          "cwd": ".."
        }
      ]
    }
  ]
}
